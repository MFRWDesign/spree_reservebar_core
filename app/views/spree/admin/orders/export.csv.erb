<% require 'csv' %>
<% if params[:type] == 'order' %>
	<%= render :partial => 'spree/admin/orders/reports/orders' %>
<% elsif params[:type] == 'product' %>
	<%= render :partial => 'spree/admin/orders/reports/products' %>
<% elsif params[:type] == 'pl_total' %>
	<%= render :partial => 'spree/admin/orders/reports/pl_total' %>
<% elsif params[:type] == 'pl_detail' %>
	<%= render :partial => 'spree/admin/orders/reports/pl_detail' %>
<% end %>
